--this is a killbrick script place inside which part u wana make a killscript :))
local part = script.Parent

part.Touched:Connect(function(hit)
	local character = hit.Parent
	if character then
		local humanoid = character:FindFirstChildWhichIsA("Humanoid")
		if humanoid then
			humanoid.Health = 0 --u can change to if u want to make how much dmg to player:))
		end
	end
end)
